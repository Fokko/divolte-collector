// Mincode/JSON samples.
[
  {
    "title": "Value: String",
    "json": "a string",
    "code": "sa string!"
  },
  {
    "title": "Value: String that requires escaping",
    "json": "Hello World!",
    "code": "sHello World~!!"
  },
  {
    "title": "Value: Boolean True",
    "json": true,
    "code": "t"
  },
  {
    "title": "Value: Boolean False",
    "json": false,
    "code": "f"
  },
  {
    "title": "Value: Null",
    "json": null,
    "code": "n"
  },
  {
    "title": "Value: Float",
    "json": 10.3,
    "code": "j10.3!"
  },
  {
    "title": "Empty object",
    "json": {},
    "code": "o"
  },
  {
    "title": "Empty array",
    "json": [],
    "code": "a."
  },
  {
    "title": "Simple object",
    "json": {
      "foo": "bar",
      "baz": "daz"
    },
    "code": "Sfoo!bar!sbaz!daz!."
  },
  {
    "title": "Simple array",
    "json": [
      "foo",
      "bar",
      "baz",
      "daz"
    ],
    "code": "asfoo!sbar!sbaz!sdaz!."
  },
  {
    "title": "Array of objects",
    "json": [
      { "foo": "bar", "baz": "daz" },
      { "daz": "huh", "doo": "wha" },
      { "daz": 2,     "doo": 3     }
    ],
    "code": "aSfoo!bar!sbaz!daz!.Sdaz!huh!sdoo!wha!.Ddaz!2!ddoo!3!.."
  },
  {
    "title": "Example from https://github.com/Sage/jsurl",
    "json": {"name":"John Doe","age":42,"children":["Mary","Bill"]},
    "code": "Sname!John Doe!dage!16!achildren!sMary!sBill!.."
  },
  {
    "title": "Object with nested objects, arrays and mixed content",
    "json": {
      "a": {},
      "b": "c",
      "d": {
        "a": [],
        "b": "g"
      },
      "e": [
        "1",
        "2"
      ],
      "f": 42,
      "g": 53.2,
      "h": -37,
      "i": -78.3e10,
      "j": {}
    },
    "code": "Oa!sb!c!Ad!a!.sb!g!.ae!s1!s2!.df!16!jg!53.2!dh!-11!ji!-78.3e10!oj!."
  }
]
